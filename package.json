{
  "name": "next-level-real-estate",
  "version": "1.0.0",
  "description": "AI-powered real estate wholesale platform with multi-agent intelligence",
  "private": true,
  "workspaces": [
    "services/*",
    "agents/*",
    "shared/*"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:gateway": "npm run dev --workspace=services/api-gateway",
    "dev:lead": "npm run dev --workspace=services/lead-service",
    "dev:agents": "npm run dev --workspace=agents/shared",
    "build": "npm run build --workspaces",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "twilio": "^5.10.4",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  }
}
