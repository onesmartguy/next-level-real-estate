# Server Configuration
PORT=3001
NODE_ENV=development

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017
MONGODB_DB_NAME=next_level_real_estate

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=lead-service
KAFKA_GROUP_ID=lead-service-group

# OpenTelemetry Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318/v1/traces
OTEL_SERVICE_NAME=lead-service

# Webhook Secrets
ZILLOW_WEBHOOK_SECRET=your-zillow-secret-here
GOOGLE_ADS_WEBHOOK_SECRET=your-google-ads-secret-here

# RealGeeks API Credentials
REALGEEKS_API_USERNAME=your-realgeeks-username
REALGEEKS_API_PASSWORD=your-realgeeks-password

# DNC Registry Configuration (optional)
# DNC_API_KEY=your-dnc-api-key
# DNC_API_ENDPOINT=https://api.dnc-registry.com
DNC_SCRUB_INTERVAL_DAYS=31

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
